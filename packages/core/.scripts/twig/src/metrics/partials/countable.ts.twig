{{metricMethod}}(options?: CountableMetricOptions): void {
  const {adapter, delta, metric, tags} = {
    ...{
      delta: 1,
    },
    ...(options || {}),
  } as CountableMetricOptions;

  const adapters = this.{{metricType}}Adapters(adapter, metric);
  adapters.forEach(({{metricType}}) => {
    {{metricType}}.{{metricMethod}}({delta, tags});
  });
}
